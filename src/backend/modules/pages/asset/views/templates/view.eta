<% it.meta = {
  title: 'Godot Library | Asset',
  description: 'Find the best Godot Assets on Godot Library',
  url: 'https://godotlibrary.com',
  moduleName: 'asset'
} %>
<% layout('templates/components/layouts/promobar-nav-body-footer.eta') %>

<section class="page-asset">
  <div class="column">
    <div class="media">
      <div class="thumbnails">

      </div>
      <div class="player">

      </div>
    </div>
    <div class="info">
      <div class="tabs">
        <ul>
          <li>Description</li>
          <li>Reviews</li>
          <li>Changelog</li>
          <li>File Tree</li>
          <li>Author Information</li>
        </ul>
      </div>
    </div>
  </div>
  <div class="column">
    <h2>Quick Information</h2>
    <div class="rating">
      <ul class="stars" data-rating="3.5">
        <li class="stars__item"></li>
        <li class="stars__item"></li>
        <li class="stars__item"></li>
        <li class="stars__item"></li>
        <li class="stars__item"></li>
      </ul>
      <span>14 ratings</span>
    </div>
    <div class="card">

      <div class="banner">
        <div class="badges">
          <div class="badge badge-blue">2D TOOLS</div>
          <div class="badge badge-purple">3.2</div>
          <span class="iconify" data-icon="akar-icons:more-horizontal"></span>
        </div>
        <% if (!it?.featured) { %>
        <div class="background">
          <div class="mask">
            <img
              src="https://images.unsplash.com/photo-1644146083026-ee18d9f05a53?ixlib=rb-1.2.1&ixid=MnwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8&auto=format&fit=crop&w=1171&q=80"
              alt="">
          </div>
        </div>
        <% } %>
      </div>
      <div class="body">
        <div class="top">
          <div class="icon">
            <img
              src="data:image/png;base64,"
              alt="">
          </div>
          <div class="info">
            <div class="author">LITTLEMOUSEDEV</div>
            <div class="title">Whiskers Dialogue Editor</div>
          </div>
        </div>
        <p>When your player enters a specific node, show greeting text</p>
        <div class="specs">
          <div class="row">
            <div class="property">Supported Engine Version</div>
            <div class="value">3.2.x</div>
          </div>
          <div class="row">
            <div class="property">Latest Version</div>
            <div class="value">1.0.2</div>
          </div>
        </div>
        <button>Add to Project</button>
      </div>
    </div>
  </div>
</section>
